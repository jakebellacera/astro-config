{
  "name": "astro-config",
  "version": "0.0.1",
  "description": "Creates an importable configuration for your Astro site or integration",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "astro",
    "astro-integration",
    "config"
  ],
  "author": "Jake Bellacera",
  "license": "MIT",
  "peerDependencies": {
    "astro": "^4.0.0"
  },
  "devDependencies": {
    "astro": "^4.0.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
